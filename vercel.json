{
    "builds":[
        {
            "src": "backend/wsgi.py",
            "name": "Personality Predictor Backend",
            "use": "@vercel/python",
            "config" : {"maxLambdaSize":"15mb","runtime":"python3.9"}
        }
    ],
    "routes":[
        {
            "src": "/(.*)",
            "dest": "backend/wsgi.py"
        }
    ]
}